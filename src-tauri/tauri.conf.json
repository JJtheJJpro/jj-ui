{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "jj-ui",
    "version": "0.0.0",
    "identifier": "com.jj-ui.app",
    "build": {
        "beforeDevCommand": "pnpm dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "jj-ui",
                "fullscreen": false,
                "resizable": false,
                "decorations": false,
                "alwaysOnBottom": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.ico"
        ]
    }
}